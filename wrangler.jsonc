{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ai-blog-cms",
  "compatibility_date": "2025-11-22",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "ai-blog-cms-db",
      "database_id": "local-only"
    }
  ]
}